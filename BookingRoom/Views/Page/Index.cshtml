@model BookingRoom.Models.Page

@{
    ViewBag.Title = Model.Title;
    Layout = "~/Views/Shared/Home/_HomeLayout.cshtml";
}
<section class="container-fluid bg-white py-5">
    <div class="container">
        <div class="row">
            <div class="col-12 col-md-9 bg-white">
                <h2 class="text-center mb-4 text-primary">@Model.Title</h2>
                <hr class="bg-dark" width="60" style="height:2px" />
                @Html.Raw(Model.Content)
            </div>
            @Html.Partial("Home/_SidebarBlogPartial")
        </div>
    </div>
</section>
